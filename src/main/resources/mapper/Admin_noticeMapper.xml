<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wxy.dao.Admin_noticeDao">

    <select id="getNoticeAll" resultType="com.wxy.pojo.Admin_notice">
        select * from admin_notice
    </select>

    <insert id="addNoticeAll" parameterType="com.wxy.pojo.Admin_notice">
        insert into admin_notice values (#{nid},#{context},#{time})
    </insert>

</mapper>